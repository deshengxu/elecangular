{
  "name": "elecangular",
  "version": "0.1.0",
  "description": "Using Angular 2 with Electron",
  "main": "main.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run cpx\" \"electron .\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "cpx": "cpx \"src/**/*.{html,css,js,png,jpg}\" dist --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xudesheng/elecangular.git"
  },
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "Angular",
    "Angular2",
    "Angular 2",
    "Xu Desheng",
    "Desheng"
  ],
  "author": "Xu Desheng<xudesheng@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xudesheng/elecangular/issues"
  },
  "homepage": "https://github.com/xudesheng/elecangular/",
  "dependencies": {
    "@angular/common": "2.2.0",
    "@angular/compiler": "2.2.0",
    "@angular/core": "2.2.0",
    "@angular/forms": "2.2.0",
    "@angular/http": "2.2.0",
    "@angular/platform-browser": "2.2.0",
    "@angular/platform-browser-dynamic": "2.2.0",
    "@angular/router": "3.2.0",
    "@angular/upgrade": "2.2.0",
    "JSON": "^1.0.0",
    "core-js": "2.4.1",
    "express": "^4.15.2",
    "json-loader": "^0.5.4",
    "jsonfile": "^3.0.0",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "zone.js": "0.6.25"
  },
  "devDependencies": {
    "@types/core-js": "0.9.34",
    "@types/node": "6.0.45",
    "concurrently": "3.0.0",
    "typescript": "2.2.1",
    "electron": "1.6.2",
    "cpx": "1.5.0"
  }
}
